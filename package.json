{
    "name": "music",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "core-js": "^2.6.5",
        "hls.js": "^0.12.4",
        "muse-ui": "^3.0.2",
        "muse-ui-toast": "^0.3.0",
        "vue": "^2.6.10",
        "vue-router": "^3.0.3",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.10.0",
        "@vue/cli-plugin-eslint": "^3.10.0",
        "@vue/cli-service": "^3.10.0",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "lib-flexible": "^0.3.2",
        "node-sass": "^4.13.1",
        "postcss-px2rem": "^0.3.0",
        "px2rem-loader": "^0.1.9",
        "sass-loader": "^7.1.0",
        "vue-template-compiler": "^2.6.10"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/standard"
        ],
        "rules": {
            "space-before-function-paren": [
                0,
                "always"
            ],
            "indent": 0,
            "semi": 0,
            "quotes": 0,
            "eol-last": 0
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {
                "browsers": [
                    "Android >= 4.0",
                    "iOS >= 7"
                ]
            }
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}